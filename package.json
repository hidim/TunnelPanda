{
  "name": "tunnelpanda",
  "version": "1.0.0",
  "description": "A tiny Node reverse-proxy for exposing Ollama API via Cloudflare Tunnel with Basic Auth.",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "setup": "node src/setup.js",
    "stop": "curl -X POST http://localhost:16014/control/stop",
    "restart": "curl -X POST http://localhost:16014/control/restart",
    "status": "curl -X POST http://localhost:16014/control/status",
    "logs": "curl -X POST http://localhost:16014/control/logs"
  },
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.0.0",
    "basic-auth": "^2.0.1",
    "axios": "^1.6.0",
    "dotenv": "^16.4.5"
  },
  "author": "",
  "license": "MIT"
}
